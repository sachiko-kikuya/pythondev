   ],
   "source": [
    "price = int(input(\"税抜価格を入力してください：\"))\n",
    "price_tax = int(round(price * 1.1, 0))\n",
    "\n",
    "if price_tax >= 2000:\n",
    "    print(\"送料は無料です\")\n",
    "else:\n",
    "    print(\"送料として350円かかります\")\n",
    "    price_tax = price_tax + 350\n",
    "\n",
    "print(f\"送料込みの価格は{price_tax}円です。\")\n"
   ]
